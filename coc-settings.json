{
  "workspace.ignoredFolders": ["$HOME", "$HOME/.cargo/**", "$HOME/.rustup/**"],
  "suggest.noselect": true,
  "html.format.enable": false,
  "languageserver": {
    "cmake": {
      "command": "cmake-language-server",
      "filetypes": ["cmake"],
      "rootPatterns": ["build/"],
      "initializationOptions": {
        "buildDirectory": "build"
      }
    },
    "sourcekit-lsp": {
      "filetypes": ["swift"],
      "command": "sourcekit-lsp"
    }
  },
  "coc.preferences.extensionUpdateCheck": "daily",
  "snippets.ultisnips.pythonPrompt": false,
  "[typescript][json][html][javascript]": {
    "coc.preferences.formatOnSave": true
  },
  "snippets.extends": {
    "dart": ["dart", "flutter"]
  },
  "sql.formatOptions": {
    "keywordCase": "upper",
    "tabWitdh": 2,
    "dataTypeCase": "upper",
    "functionCase": "upper"
  },
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/18.1.3/clangd_18.1.3/bin/clangd",
  "python.linting.pylintPath": "/Users/kyungsubkim/.local/bin/pylint",
  "java.jdt.ls.directory": "/Users/kyungsubkim/jdtls",
  "java.jdt.ls.java.home": "/usr/local/opt/openjdk@17",
  "java.jdt.ls.statusIcons": {
    "busy": "Busy",
    "ready": "OK",
    "warning": "Warning",
    "Error": "Error"
  },
  "java.import.gradle.version": "8.7",
  "java.import.gradle.home": "/usr/local/Cellar/gradle/8.7",
  "java.import.gradle.java.home": "/usr/local/opt/openjdk@17",
  "java.completion.filteredTypes": [
    "com.sun.*",
    "sun.*",
    "jdk.*",
    "org.graalvm.*",
    "io.micrometer.shaded.*"
  ],
  "xml.java.home": "/usr/local/opt/openjdk@17"
}
