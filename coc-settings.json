{
  "workspace.ignoredFolders": ["$HOME", "$HOME/.cargo/**", "$HOME/.rustup/**"],
  "suggest.noselect": true,
  "html.format.enable": false,
  "languageserver": {
    "go": {
      "command": "gopls",
      "args": ["serve", "-debug", "0.0.0.0:8484", "-rpc.trace"],
      "rootPatterns": ["go.mod", "go.work", ".vim/", ".git/"],
      "trace.server": "verbose",
      "filetypes": ["go"],
      "initializationOptions": {}
    },
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gd", "gdscript", "gdscript3"],
      "port": 6005
    },
    "sourcekit-lsp": {
      "filetypes": ["swift"],
      "command": "sourcekit-lsp"
    }
  },
  "coc.preferences.extensionUpdateCheck": "daily",
  "snippets.ultisnips.pythonPrompt": false,
  "[typescript][json][html][javascript]": {
    "coc.preferences.formatOnSave": true
  },
  "snippets.extends": {
    "dart": ["dart", "flutter"]
  },
  "sql.formatOptions": {
    "keywordCase": "upper"
  },
  "sql.database": "postgresql",
  "java.import.gradle.java.home": "/usr/local/Cellar/openjdk@17/17.0.9/libexec/openjdk.jdk/Contents/Home",
  "java.saveActions.organizeImports": true,
  "java.completion.filteredTypes": []
}
